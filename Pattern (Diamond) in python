def pattern_diamond():
	x = 10
	for i in range(1, 12, 2):
		print x*' ' ,i*'*'
		x = x - 1
	y = 5
	for a in range(11, 0, -2):
		print y*' ', a*'*'
		y += 1

def how_many_no.of_diamond(d):
	q = 0
	while q < d:
		x = 10
		for i in range(1, 12, 2):
			print x*' ' ,i*'*'
			x = x - 1
		y = 5
		for a in range(11, 0, -2):
			print y*' ', a*'*'
			y += 1
		q += 1
